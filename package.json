{
  "name": "service-desk-plus-cloud-api",
  "version": "1.0.0",
  "description": "Service Desk Plus Cloud API integration with MCP server for AI assistants",
  "type": "module",
  "main": "dist/exports.js",
  "types": "dist/exports.d.ts",
  "bin": {
    "sdp-mcp-server": "dist/index.js",
    "sdp-mcp-sse": "dist/indexSSE.js",
    "sdp-mcp-multiuser": "dist/indexSSEMultiUser.js",
    "sdp-mcp-registry": "dist/indexSSEWithRegistry.js",
    "sdp-mcp-self-client": "dist/indexSSESelfClient.js",
    "sdp-user-admin": "dist/cli/userAdmin.js"
  },
  "exports": {
    ".": {
      "import": "./dist/exports.js",
      "types": "./dist/exports.d.ts"
    },
    "./optimized": {
      "import": "./dist/api/optimizedClient.js",
      "types": "./dist/api/optimizedClient.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "node --loader ts-node/esm src/index.ts",
    "dev:v2": "node --loader ts-node/esm src/indexV2.ts",
    "dev:v3": "node --loader ts-node/esm src/indexV3.ts",
    "dev:v4": "node --loader ts-node/esm src/indexV4.ts",
    "dev:sse": "node --loader ts-node/esm src/indexSSE.ts",
    "dev:multiuser": "node --loader ts-node/esm src/indexSSEMultiUser.ts",
    "dev:registry": "node --loader ts-node/esm src/indexSSEWithRegistry.ts",
    "dev:self-client": "node --loader ts-node/esm src/indexSSESelfClient.ts",
    "start": "node dist/index.js",
    "start:v2": "node dist/indexV2.js",
    "start:v3": "node dist/indexV3.js",
    "start:v4": "node dist/indexV4.js",
    "start:sse": "node dist/indexSSE.js",
    "start:multiuser": "node dist/indexSSEMultiUser.js",
    "start:registry": "node dist/indexSSEWithRegistry.js",
    "start:self-client": "node dist/indexSSESelfClient.js",
    "user:admin": "node dist/cli/userAdmin.js",
    "user:setup": "node scripts/setup-users.js",
    "oauth:setup": "node scripts/oauth-setup.js",
    "test": "jest",
    "test:rate-limit": "node dist/scripts/test-rate-limit-coordinator.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "service-desk-plus",
    "api",
    "manageengine",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/pg": "8.15.4",
    "@types/uuid": "^10.0.0",
    "async-mutex": "^0.4.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventsource": "^4.0.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "pg": "8.16.3",
    "uuid": "^11.1.0",
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.0",
    "table": "^6.8.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
